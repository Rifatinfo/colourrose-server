model UiCategory {
  id        String   @id @default(uuid())
  name      String
  avatar    String?  
  isActive  Boolean  @default(true)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model UiCategoryMap {
  id              String @id @default(uuid())
  uiCategoryId    String
  realCategoryId  String

  createdAt DateTime @default(now())
}
